		"apollo-server-express": "^3.13.0",
		"bcrypt": "^5.1.1",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.3.2",
		"express": "^4.18.2",
		"graphql": "^16.8.1",
		"graphql-scalars": "^1.23.0",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^8.1.0",
		"react-hook-form": "^7.51.3"
	},
