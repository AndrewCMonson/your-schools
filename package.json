{
	"name": "full-stack-vite-project",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"server": "node --loader ts-node/esm backend/server.ts",
		"build": "yarn install && cd frontend && yarn install && yarn run build",
		"render-build": "yarn install && yarn run build",
		"client": "cd frontend && yarn run start",
		"dev": "concurrently \"yarn run server\" \"yarn run client\"",
		"data:import": "node --loader ts-node/esm backend/seeder.ts",
		"data:destroy": "tsx backend/seeder.ts -d",
		"lint": "eslint . --ext js,ts,tsx --report-unused-disable-directives --max-warnings 0",
		"generate": "graphql-codegen --config codegen.ts",
		"watch": "graphql-codegen -w"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
