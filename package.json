		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/node": "^20.12.4",
		"@types/nodemailer": "^6.4.15",
		"@typescript-eslint/eslint-plugin": "^7.4.0",
